{
  "expo": {
    "name": "EventEasy",
    "slug": "event-easy",
    "version": "1.1.0",
    "scheme": "event-easy",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#141824"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#141824"
      },
      "package": "com.ashishexpodev.EventEasy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e44bce34-6109-46da-a243-683189b3703f"
      }
    }
  }
}
